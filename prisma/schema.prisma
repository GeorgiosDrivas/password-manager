generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  name  String
  password String  @unique
}

model Item {
  id          Int      @id @default(autoincrement())
  title       String
  username    String?
  password    String?
  url         String?
  userId      Int
}